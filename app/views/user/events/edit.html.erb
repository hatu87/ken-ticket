  <div class="container-fluid tickets-buy">
    <div class="row">
      <div class="col-md-6">
        <h1 class="display-4">Edit Event</h1>
        <%= render partial: 'form', locals: {url: user_event_path, method: :put, is_edit: true} %>
      </div>
      <div class="col-md-6">
        <h1 class="display-4">Ticket Types</h1>
        <%= render(partial: 'ticket_type', collection: @event.ticket_types) || 'No ticket types' %>
          <div class="clearfix"></div>
        <%= render partial: 'ticket_type_form' %>
      </div>
    </div>
  </div>
